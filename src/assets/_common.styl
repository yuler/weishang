/*init*/
html{background:none repeat scroll 0 0#FFFFFF;color:#000000;}
html,body{height: 100%;font-family:PingFangSC-Regular,Helvetica,"Droid Sans",Arial,sans-serif;}
body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,fieldset,legend,input,textarea,p,blockquote,th,td{margin:0;padding:0;}
table{border-collapse:collapse;border-spacing:0;}
fieldset,img{border:0 none;}
address,caption,cite,code,dfn,em,strong,th,var{font-style:normal;font-weight:normal;}
li{list-style:none outside none;}
caption,th{text-align:left;}
h1,h2,h3,h4,h5,h6{font-size:100%;font-weight:normal;}
q:before,q:after{content:"";}
abbr,acronym{border:0 none;font-variant:normal;}
sup{vertical-align:text-top;}
sub{vertical-align:text-bottom;}
input,textarea,select{font-family:inherit;font-size:inherit;font-weight:inherit;}
input,textarea,select{}
legend{color:#000000;}
body{font:13px/1.231 arial,helvetica,clean,sans-serif;}
select,input,button,textarea{font:99%arial,helvetica,clean,sans-serif;}
table{font-size:inherit;}
pre,code,kbd,samp,tt{font-family:monospace;line-height:100%;}
a{text-decoration:none;}
a:hover,a:focus{text-decoration:underline;}
strong{font-weight:bold;}
input[type="submit"]{cursor:pointer;}
button{cursor:pointer;}


.my-user{
  position: absolute;
  right: 10px;
  top: 10px;
  font-size: 18px;
}
.viewports{
  width: 100%;
  height: 100%;
  font-size: 0px;
  max-width: 640px;
  min-width: 320px;
  margin: 0 auto;
  /* overflow-x: hidden; */
}
.container{
  width: 100%;
  height: 100%;
  /* overflow-x: hidden; */
}
.btn{
  display: inline-block;
  padding: 0px 10px;
  background-color: #dfbd98;
  font-size: 15px;
  color: #ffffff;
  border-radius: 5px;
  text-align: center;
}
ul,li{
  list-style: none;
}
.btn-info{
  padding-top: 2px;
  padding-bottom: 2px;
}
/*分割线*/
.split-line{
  border-bottom: 1px solid #e1e1e1;
  border-width: 0 0 1px 0;
  -webkit-border-image: url("data:image/gif;base64,") 2 0 stretch;
}

/*分割线*/
.split-line-up{
  border-top: 1px solid #e1e1e1;
  border-width: 1px 0 0 0;
  -webkit-border-image: url("data:image/gif;base64,") 2 0 stretch;
}
.margin-space{
  margin-left: 18px;
  padding-right: 18px;
  padding-bottom: 18px;
  padding-top: 18px;
}
.margin-space-s{
  margin-left: 10px;
  padding-right: 10px;
  margin-top: 10px;
}
.margin-l{
  margin-left: 18px;
}
.margin-r{
  margin-right: 18px;
}
.margin-t{
  margin-top: 18px;
}
.floor-item{
  width: 100%;
  background-color: white;
}
.floor-space:after{
  content: ' ';
  display: inline-block;
  width: 100%;
  height: 10px;
  background-color: #E2E2E2;
}
.title-pannel{
  width: 100%;
  font-size: 16px;
  height: 40px;
  line-height: 40px;
}
.title-pannel .title-item{
  float: left;
  height: 100%;
  text-align: center;
  background-color: #DADADA;
}
.current-active{
  background-color: #ffffff!important;
  border-bottom: solid 2px #dfbd98;
  color: #dfbd98;
}
.user-pannel{font-size: 16px;color: #666666;margin-top: 10px;margin-left: 70px;}
.user-pannel i{font-style: normal;width: 16px;height: 16px;display: inline-block;}
.full-btn{
  color: #fff;
  height: 44px;
  width: 100%;
  font-size: 14px;
  border: 1px solid #dfbd98;
  border-radius: 4px;
  background: #dfbd98;
}

.fa-trash-o:before{
  font-size: 20px;
  color: #bf1414;
}
.icon.add:before{
  font-size: 18px;
  color: #bf1414;
}
.weishang-icon{
  background: url(/static/i/weishang-icon.png) no-repeat;
  background-size: 200px 200px;
}